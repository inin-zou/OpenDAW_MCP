{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/mcp.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/mcp(.*)",
      "dest": "/api/mcp.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "AWS_REGION": "eu-north-1",
    "S3_BUCKET": "musixtral"
  }
}
